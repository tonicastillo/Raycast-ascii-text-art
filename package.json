{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ascii-text-art",
  "title": "ASCII Text Art",
  "description": "Convert any text to ASCII text art like this one.\n",
  "icon": "command-icon.png",
  "author": "tonicastillo",
  "owner": "toni_castillo",
  "categories": [
    "Design Tools",
    "Developer Tools",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "asciitext",
      "title": "Asciitext",
      "subtitle": "Generate ASCII ",
      "description": "Convert any text in ASCII Art",
      "mode": "view"
    },
    {
      "name": "asciitextconfig",
      "title": "AsciitextConfig",
      "subtitle": "Configure",
      "description": "Select Figlet font",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}